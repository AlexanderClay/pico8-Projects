function draw_actor(a)
	if (a.active) then
	
	
  if a.frames > 1 then
   a.frame %= a.t
  end
  if (a == g1) then
   debugm = "asdf"
  end
  a.t += 1
  
	
		local dim = vec2(a.gdir*a.sprsw*0.5, a.sprsh*0.5)
		
		-- locaiton in game
		local sx = (a.x * 8)-(mapx*128) - dim.x
  local sy = (a.y * 8)-(mapy*120) - dim.y+8 -- game ui offset

 	-- frame from animation sprite
 	local sprx = flr(a.spr % 16)*8
 	local spry = flr(a.spr / 16)*8

 	sspr(sprx, spry, a.sprw, a.sprh,sx, sy, round(a.gdir*a.sprsw), a.sprsh)
  
 end
end

