-- helper functions
function lerp(a, b, t)
	return a + t * (b-a)
end
function move2(a, b, s)
	if(a > b) then return a-s
	else return a+s end
end
function round(a)
	return flr(a+0.5)
end
function textshadow(text, x, y, col, col2)
	if (col2 == nil) col2=0
	print (text, x-1, y+1, col2) -- shadow
	print (text, x, y, col) -- main
end
function text_high(text, x, y, col, col2)
	if (col2 == nil) col2=0
	print (text, x-1, y+1, col2) -- shadow
	print (text, x+1, y+1, col2) -- shadow
	print (text, x-1, y-1, col2) -- shadow
	print (text, x+1, y-1, col2) -- shadow
	print (text, x-1, y, col2) -- shadow
	print (text, x+1, y, col2) -- shadow
	print (text, x, y+1, col2) -- shadow
	print (text, x, y-1, col2) -- shadow
	print (text, x, y, col) -- main
end


