-- make an actor
-- and add to global collection
-- x,y means center of the actor
-- in map tiles (not pixels)
function make_actor(x, y)
 a={}
 a.dir=1
 a.gdir=1
 a.active=false
 a.x = x
 a.y = y
 a.dx = 0
 a.dy = 0
 a.spr = 16
 a.sprw = 8 -- sprite width
 a.sprh = 8
 a.sprsw = 8 -- sprite screen width
 a.sprsh = 8
 a.frame = 0
 a.frames = 0
 a.t = 0
 a.moveable = 0
 a.drag = 0.6
 
 -- half-width and half-height
 -- slightly less than 0.5 so
 -- that will fit through 1-wide
 -- holes.
 a.w = 0.44
 a.h = 0.44
 
 add(actor,a)
 
 return a
end
function make_item(x, y)
 local obj=make_actor(x,y)
 obj.name=""
 obj.active=true
 obj.uw = 1 -- use-width
 obj.uh = 1
 obj.w=0.1
 obj.h=0.1
 obj.moveable = 0.3
 obj.active=true
 obj.update=function()
 	if (abs(obj.dx)+abs(obj.dy) > 0.005) then
   sfx(40, 0)
  end
 end
 add(use_actors, obj)
 add(solid_actors, obj)

 return obj
end

-- for any given point on the
-- map, true if there is wall
-- there.

function solid(x, y)

 -- grab the cell value
 val=mget(x, y)
 
 -- check if flag 1 is set (the
 -- orange toggle button in the 
 -- sprite editor)
 return fget(val, 1)
 
end

-- solid_area
-- check if a rectangle overlaps
-- with any walls

--(this version only works for
--actors less than one tile big)

function solid_area(x,y,w,h)

 return 
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end

function update_actors()
	for a in all(actor) do
		if (a.update != nil) then
			a.update()
		end
	end
end
-- true if a will hit another
-- actor after moving dx,dy
function detect_use()
	for a2 in all(use_actors) do
		local x=pl.x-a2.x
		local y=pl.y-a2.y
		if ((abs(x) < (a.w+a2.uw) and
	 (abs(y) < a.h+a2.uh)))
		then
			return a2
		end
	end
	
	return false	
end

function hurt(obj,dam)
	if(obj == pl) then
  pal(7, 8)
	end
	if(obj.hp != nil) then
		obj.hp -= dam
		if(obj.hp <= 0) then
 	 obj.active = false
 	 del(actor,obj)
 	 del(solid_actors,obj)
 	 if(obj.kill) then
 	 	obj.kill()
 	 end
 	end
	end
end
function solid_actor(a, dx, dy)
	
	if(a.active) then
	
  for a2 in all(solid_actors) do
   if a2 != a then
    local x=(a.x+dx) - a2.x
    local y=(a.y+dy) - a2.y
    if ((abs(x) < (a.w+a2.w)) and
     (abs(y) < (a.h+a2.h)))
    then 
    
    if(a.layer == 1) then -- bullets
    -- if shot by player and inside player
    	if(a2 == pl and a.tag=="pl") then
    	 return false
    	end
    	hurt(a2,a.dam)
    	a.active=false
    elseif (a.layer == 3) then -- teleporter bullet
    	if(a2 == pl) then
    	 return false
    	end
    	sfx(22,3)
    	a.active=false
    	pl.active=true
    	pl.x=a2.x
    	pl.y=a2.y
    	hurt(a2,100)
    end
     -- moving together?
     -- this allows actors to
     -- overlap initially 
     -- without sticking together    
     if (dx != 0 and abs(x) <
         abs(a.x-a2.x)) then
      v=a.dx + a2.dy
      a.dx = v/2 * a.moveable
      a2.dx = v/2 * a2.moveable
      return true
     end
     
     if (dy != 0 and abs(y) <
         abs(a.y-a2.y)) then
      v=a.dy + a2.dy
      a.dy=v/2 * a.moveable
      a2.dy=v/2 * a2.moveable
      return true
     end
     
     --return true
    end
   end
  end
 return false
 end
end


-- checks both walls and actors
function solid_a(a, dx, dy)
 if solid_area(a.x+dx,a.y+dy,
    a.w,a.h) then
    return true end
 return solid_actor(a, dx, dy) 
end

function move_actor(a)

	if (a.active) then
  -- only move actor along x
  -- if the resulting position
  -- will not overlap with a wall
 
  if not solid_a(a, a.dx, 0) 
  then
   a.x += a.dx
  else   
   if(a.layer == 1) then -- bullets
   	a.active=false
   elseif(a.layer == 3) then -- telebullet
   	sfx(22,3)
   	a.active=false
   	pl.active=true
   	pl.x=a.x
   	pl.y=a.y
   end
  end
 
  -- ditto for y
 
  if not solid_a(a, 0, a.dy) then
   a.y += a.dy
  else
   if(a.layer == 1) then -- bullets
   	a.active=false
   end
  end
  
  -- apply drag
  -- set dx,dy to zero if you
  -- don't want drag
  
  a.dx *= a.drag
  a.dy *= a.drag
  
 end
end
