function setroom(x,y)
	mapx=x
	mapy=y
	
	for tx=x*16,x*16+16 do
	 for ty=y*15,y*15+15 do
	 
	  -- enemy1
	 	if(mget(tx,ty)==11) then
    local enmy1=make_actor(tx+0.5,ty+0.5)
    enmy1.spr=11
    enmy1.dx = -0.2
    enmy1.drag = 1
    enmy1.hp=10
    enmy1.layer = 2
    enmy1.active=true
    add(solid_actors,enmy1)
    enmy1.update = function()
    	enmy1.sprsw = 8 + sin(enmy1.x*0.8)
    	enmy1.sprsh = 8 + sin(enmy1.y)
    end
	 	elseif(mget(tx,ty)==64) then
	 		sfx(60,0)
 	 	chase = make_actor(tx,ty)
    chase.active=true
    chase.spr = 64
    chase.w = 0.5
    chase.h = 0.5
    chase.sprsw = 16
    chase.sprsh = 16
    chase.sprw = 16
    chase.sprh = 16
    chase.layer = 2
    chase.cool = 5
    chase.update = function()
    	chase.sprsw = 16 + sin(chase.t*0.05)*2
    	chase.sprsh = 16 + sin(chase.t*0.06)*2
    	
    	if (pl.x > chase.x+0.1) then
    		chase.dir = 1
    	else chase.dir=-1 end
    	chase.gdir = lerp(chase.gdir, chase.dir, 0.5)
    	
    	 
    	chase.x = move2(chase.x,pl.x,0.1)
    	chase.y = move2(chase.y,pl.y,0.1)
    end
    
	 	-- gun1
	 	elseif(mget(tx,ty)==21) then
    local g1=make_item(tx+0.5,ty+0.5)
    g1.spr = 21
    g1.use = function()
    	sfx(10,1)
    	del(actor, g1)
    	del(use_actors, g1)
   	 del(solid_actors, g1)
   	 g1=nil
    	pl.g=gun1
    	sfx(2,0)
    end
    
	 	-- gun2
	 	elseif(mget(tx,ty)==22) then
    local g1=make_item(tx+0.5,ty+0.5)
    g1.spr = 22
    g1.frames = 2
    g1.use = function()
