  rect(5, 40, 122, 90, 7)
  rectfill(6,41,121,89,1)
  rect(7, 42, 120, 88, 7)
  textshadow("> press 'e' to proceed", 20, 80, 7)
  textshadow(msg1, 15, 50, 7)
  textshadow(msg2, 15, 65, 8)
	end
end

function textshadow(text, x, y, col)
	print (text, x-1, y+1, 0) -- shadow
	print (text, x, y, col) -- main
end
