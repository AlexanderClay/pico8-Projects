use_actors = {}
solid_actors = {}
actor = {} --all actors in world
paused = false
msg1= "you broke your all your\nfingers."
msg2= "(hp -1 on every shot)"
mapx=0
mapy=0
-- make an actor
-- and add to global collection
-- x,y means center of the actor
-- in map tiles (not pixels)
function make_actor(x, y)
 a={}
 a.active=false
 a.moveable = 1
 a.x = x
 a.y = y
 a.dx = 0
 a.dy = 0
 a.spr = 16
 a.sprw = 8 -- sprite width
 a.sprh = 8
 a.sprsw = 8 -- sprite screen width
 a.sprsh = 8
 a.frame = 0
 a.t = 0
 a.drag = 0.6
 a.bounce  = 0
 a.frames=2
 
 -- half-width and half-height
 -- slightly less than 0.5 so
 -- that will fit through 1-wide
 -- holes.
 a.w = 0.44
 a.h = 0.44
 
 add(actor,a)
 
 return a
end

function setroom(x,y)
	mapx=x
	mapy=y
	for tx=0,16 do
	 for ty=0,16 do
	  -- eyeball monster
	 	if(mget(tx,ty)==11) then
	 		mset(tx,ty, 0)
    local enmy1=make_actor(tx+0.5,ty+0.5)
    enmy1.spr=11
    enmy1.dx = -0.2
    enmy1.drag = 1
    enmy1.layer = 2
    enmy1.active=true
    
    enmy1.update = function()
    	enmy1.sprsw = 8 + sin(enmy1.x*0.8)
    	enmy1.sprsh = 8 + sin(enmy1.y)
    end
    add(solid_actors, enmy1)
	 	end
	 	-- car
	 	if(mget(tx,ty)==56) then
	 		mset(tx,ty, 0)
    local car=make_actor(tx+0.5,ty+0.5)
    car.name="  press 'e' to use car"
    car.uw = 1
    car.moveable = 0.2
    car.uh = 1
    car.spr = 56
    car.active=true
    car.update=function()
    	if (abs(car.dx)+abs(car.dy) > 0.005) then
      sfx(40, 3)
     end
    end
    add(use_actors, car)
    add(solid_actors, car)
	 	end
	 end
	end
end

-- for any given point on the
-- map, true if there is wall
-- there.

function solid(x, y)

 -- grab the cell value
 val=mget(x, y)
 
 -- check if flag 1 is set (the
 -- orange toggle button in the 
 -- sprite editor)
 return fget(val, 1)
 
end

-- solid_area
-- check if a rectangle overlaps
-- with any walls

--(this version only works for
--actors less than one tile big)

function solid_area(x,y,w,h)

 return 
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end

function update_actors()
	for a in all(actor) do
		if (a.update != nil) then
			a.update()
		end
	end
end
-- true if a will hit another
-- actor after moving dx,dy
function detect_use()
	for a2 in all(use_actors) do
		local x=pl.x-a2.x
		local y=pl.y-a2.y
		if ((abs(x) < (a.w+a2.uw) and
		 (abs(y) < a.h+a2.uh)))
		then
			return a2.name
		end
	end
	
	return false	
end

function hurt(obj)
	if(obj == pl) then
  pal(7, 8)
	end
	if(obj.hp != null) then
		obj.hp = 0
	end
end
function solid_actor(a, dx, dy)
	
	if(a.active) then
	
  for a2 in all(solid_actors) do
   if a2 != a then
    local x=(a.x+dx) - a2.x
    local y=(a.y+dy) - a2.y
    if ((abs(x) < (a.w+a2.w)) and
     (abs(y) < (a.h+a2.h)))
    then 
    
    if(a.layer == 1) then -- bullets
    -- if shot by player and inside player
    	if(a2 == pl and a.tag=="pl") then
    	 return false
    	end
    	a.active=false
    	hurt(a2)
    end
     -- moving together?
     -- this allows actors to
     -- overlap initially 
     -- without sticking together    
     if (dx != 0 and abs(x) <
         abs(a.x-a2.x)) then
      v=a.dx + a2.dy
      a.dx = v/2 * a.moveable
      a2.dx = v/2 * a2.moveable
      return true
     end
     
     if (dy != 0 and abs(y) <
         abs(a.y-a2.y)) then
      v=a.dy + a2.dy
      a.dy=v/2 * a.moveable
      a2.dy=v/2 * a2.moveable
      return true
     end
     
     --return true
    end
   end
  end
 return false
 end
end


-- checks both walls and actors
function solid_a(a, dx, dy)
 if solid_area(a.x+dx,a.y+dy,
    a.w,a.h) then
    return true end
 return solid_actor(a, dx, dy) 
end

function move_actor(a)

	if (a.active) then
  -- only move actor along x
  -- if the resulting position
  -- will not overlap with a wall
 
  if not solid_a(a, a.dx, 0) 
  then
   a.x += a.dx
  else   
   -- otherwise bounce
   a.dx *= -a.bounce
    if(a.layer == 1) then -- bullets
    	a.active=false
    end
  end
 
  -- ditto for y
 
  if not solid_a(a, 0, a.dy) then
   a.y += a.dy
  else
   a.dy *= -a.bounce
   if(a.layer == 1) then -- bullets
    	a.active=false
    end
  end
  
  -- apply drag
  -- set dx,dy to zero if you
  -- don't want drag
  
  a.dx *= a.drag
  a.dy *= a.drag
  
  -- advance one frame every
  -- time actor moves 1/4 of
  -- a tile
  if a.frame > 1 then
   a.frame += abs(a.dx) * 4
   a.frame += abs(a.dy) * 4
   a.frame %= a.frames
  end
  
  a.t += 1
  
 end
end
function control_player(pl)

 -- how fast to accelerate
 accel = 0.07
 if (btn(0)) pl.dx -= accel 
 if (btn(1)) pl.dx += accel 
 if (btn(2)) pl.dy -= accel 
 if (btn(3)) pl.dy += accel 
	-- shootup btn(0)
	
	if(pl.cool<0) then
 	if(btn(3,1)) then
 		fire(1)
 	elseif(btn(5, 1)) then
 	
 		fire(-1)
 	end
 end
	
	-- change weapon
	-- q btn(5,1)
	-- e btn(2,1)

 
 if (abs(pl.dx)+abs(pl.dy) > 0.1
     and (pl.t%5) == 0) then
  sfx(0)
 end
 
end
function lerp(a, b, t)
	return a + t * (b-a)
end
function move2(a, b, s)
	if(a > b) then return a-s
	else return a+s end
end
function fire(dir)
 local b = bullets.spawn(pl.x, pl.y)
	b.dx=dir*0.3
	b.tag="pl"
	pl.cool = pl.g.cool
	sfx(pl.g.sfx, 3)
end
function _update()
	
	if(paused==true) then
		if(btnp(2, 1)) paused = false
		return
	end
	
	
 pl.cool-=1
 
 update_actors()
 control_player(pl)
 foreach(actor, move_actor)
end

function vec2(tx, ty)
 tmp = {
  x=tx,
  y=ty
 }
	return tmp
end

function draw_actor(a)
	if (a.active) then
		local dim = vec2(a.sprsw*0.5, a.sprsh*0.5)
		
		local sx = (a.x * 8-mapx*128) - dim.x
  local sy = (a.y * 8-mapy*128) - dim.y
  if(a.layer == 2) then
  -- todo: simplify using a.spr
  	local sprx = flr(a.spr % 16)*8
  	local spry = flr(a.spr / 16)*8
  	
  	sspr(sprx, spry, a.sprw, a.sprh,sx, sy, a.sprsw, a.sprsh)
  else
   spr(a.spr + a.frame, sx, sy)
  end
 end
end
hurt_frame = true

function _draw()
 cls()
 
 pal()
 
 --debug:
 --test if map has empty tiles:
