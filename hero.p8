function _init()
	
 pool={
 	spawn = function(x, y)
 		
  	local temp = pool[pool.i]
  	temp.active = true
  	temp.x=x
  	temp.y=y
  
  	
  	pool.i+=1
  	if (pool.i == pool.length) pool.i=1
  	
  	return temp
 	end,
 	make = function(origin)
 	 for i=1,pool.length do
   	local o = make_actor(5,5)
   	o.layer=origin.layer
    o.spr =origin.spr
    o.drag=origin.drag
    o.w =origin.w
    o.h =origin.h
    pool[i] = o
   end
  end
 }
 pool.i = 1
 pool.length = 6
 
 
 bul= make_actor(5,5)
 bul.layer=1
 bul.spr = 40
 bul.drag=1
 bul.w = 0.11
 bul.h = 0.11
 
 -- make bullets
 bullets=pool
 bullets.i=1
 bullets.length=20
 bullets.make(bul)
 
 
 gun0={}
 gun0.cool=5
 gun0.sfx = 20
 
 gun1={}
 gun1.cool=10
 gun1.sfx=24
 
 gun2={}
 gun2.cool=10
 gun2.sfx=24
 gun2.spr=22
 
 local tut=make_actor(2,4)
 tut.name="press ⬆️ ⬇️ ⬅️ ➡️ to move"
 tut.uw = 3
 tut.uh = 3
 add(use_actors, tut)
 
 chase = make_actor(6,6)
 chase.active=true
 chase.spr = 64
 chase.w = 0.5
 chase.h = 0.5
 chase.sprsw = 16
 chase.sprsh = 16
 chase.sprw = 16
 chase.sprh = 16
 chase.layer = 2
 chase.cool = 5
 chase.update = function()
 	chase.sprsw = 16 + sin(chase.x*0.8)*2
 	chase.sprsh = 16 + sin(chase.y)*2
 	chase.x = move2(chase.x,pl.x,0.1)
 	chase.y = move2(chase.y,pl.y,0.1)
 end
 
 pl = make_actor(2,4)
 pl.active=true
 pl.spr = 1
 pl.sprw = 8
 pl.sprh = 8
 pl.layer = 2
 pl.g = gun1 --weapon selection
 pl.cool = 5
 pl.hp=100
 pl.badhp=0
 pl.yhp=0
 add(solid_actors, pl)
	
	setroom(0, 0)
end
