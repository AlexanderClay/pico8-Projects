	rectfill(0, 0, 127, 8, 0)
	line(0, 9, 127, 9,7)
	line(0, 10, 127, 10,0)
	textshadow ("18/255    12:00:00    hp 100", 5, 2, 7)
 ]]
	if (detect_use() != false) then
		
  rect(5, 90, 122, 110, 7)
  rectfill(6,91,121,109,1)
  rect(7, 92, 120, 108, 7)
 	textshadow(detect_use(), 13, 98, 7)
 
		
	end
