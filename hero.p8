 --debug: mem usuage
 --print(stat(0), 20, 16)
  
 -- level1
 foreach(actor,draw_actor)
 mapx = flr(pl.x/16)
	mapy = flr(pl.y/16)
 map(mapx*16,mapy*16,0,0.5,16,16)
 
 --ui
